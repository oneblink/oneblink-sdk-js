{
  "name": "@oneblink/sdk",
  "description": "OneBlink SDK to serve as an entry point for all OneBlink Services in NodeJS",
  "version": "1.0.2",
  "author": "OneBlink <developers@oneblink.io> (https://oneblink.io)",
  "bugs": {
    "url": "https://github.com/oneblink/sdk-node-js/issues"
  },
  "dependencies": {
    "@oneblink/sdk-core": "^0.4.4",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.0.0",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@oneblink/release-cli": "^1.2.0",
    "@oneblink/types": "github:oneblink/types",
    "@types/content-disposition": "^0.5.5",
    "@types/jest": "^29.2.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fixpack": "^4.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "prettier-plugin-jsdoc": "^0.4.2",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "files": [
    "tenants"
  ],
  "homepage": "https://oneblink.github.io/sdk-node-js/",
  "keywords": [],
  "license": "MIT",
  "main": "./tenants/oneblink.js",
  "peerDependencies": {
    "aws-sdk": "^2.994.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/sdk-node-js.git"
  },
  "scripts": {
    "build": "tsc --build",
    "docs": "typedoc",
    "eslint": "eslint --fix --cache ./src ./tests",
    "fixpack": "fixpack",
    "jest": "jest --silent",
    "prepare": "npm run build",
    "pretest": "npm run eslint && tsc --noEmit && npm run fixpack",
    "release": "npm run docs && oneblink-release",
    "test": "npm run jest",
    "types": "npm i -D github:oneblink/types",
    "typescript": "tsc --noEmit"
  }
}
