    "aws-sdk": "^2.994.0",
    "content-disposition": "^0.5.3",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "node-fetch": "^2.6.5",
    "nodemailer": "^6.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@oneblink/release-cli": "^1.0.0",
    "@oneblink/types": "github:oneblink/types",
    "@types/content-disposition": "^0.5.4",
    "@types/jest": "^27.0.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fixpack": "^4.0.0",
    "jest": "^27.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.8",
    "typescript": "^4.4.3"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=8"
  },
  "files": [
    "tenants"
  ],
  "homepage": "https://oneblink.github.io/sdk-node-js/",
  "keywords": [],
  "license": "MIT",
  "main": "./tenants/oneblink.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/sdk-node-js.git"
  },
  "scripts": {
    "build": "tsc --build",
    "docs": "typedoc",
    "eslint": "eslint --fix --cache ./src ./tests",
    "fixpack": "fixpack",
    "jest": "jest --silent",
    "prepare": "npm run build",
    "pretest": "npm run eslint && tsc --noEmit && npm run fixpack",
    "release": "npm run docs && oneblink-release",
    "test": "npm run jest",
    "types": "npm i -D github:oneblink/types"
  }
